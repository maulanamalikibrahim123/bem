*{
	padding: 0;
	margin: 0;
	box-sixing: border-box;

#navbar{
	display: flex;
	align-items: center;
	justify-content: right;;
}
#navbar a{
	text-decoration: none;
	color: black;
}
#navbar li{
	list-style: none;
	padding: 0 20px;
	position: relative;
	
}
#navbar a:hover,
#navbar a.active{
	color: #088178;
	
}
#navbar a.active::after{
	content: "";
	height: 3px;
	width: 55%;
	position: absolute;
	bottom: -4px;
	left: 20px;
	background: #088178;
	
}
#navbar a:after{
	content: '';
	height: 3px;
	width: 0;
	position: absolute;
	bottom: -4px;
	left: 20px;
	background: #088178;
	transition: 0.5s;
}
#navbar a:hover:after{
	width: 55%;
}
#hero{
	background-image: linear-gradient(rgba(0,0,0,0.75),rgba(0,0,0,0.75)),url(aku.jpg);
	width: 89.6%;
	height: 100vh;
	background-position: top 60% right 0;
	background-size :cover; 
	padding: 0 70px;
	display: flex;
	align-items: flex-start;
	flex-direction: column;
	justify-content: center;
	color: white;
}
#hero h4{
	padding-bottom: 10px;
}
#hero h1{
	color: #088178;
	font-size: 35px;
	margin-top: -9px;
}
#hero h2{
	font-size: 35px;
}
#hero button{
	width: 150px;
	padding: 5px 10px;
	background: transparent;
	color: #fff;
	border-radius: 20px;
	transform: translateY(40px);
	font-size: 20px;
	position: relative;
	cursor: pointer;
	border: 1px solid #fff;
}
#hero button span{
	background: #088178;
	width: 0;
	height: 100%;
	border-radius: 18px;
	position: absolute;
	top: 0;
	left: 0;
	z-index: -1;
}
#hero button:hover span{
	width: 100%;
	transition: 0.5s;
}
#hero button:hover{
	border: none;
}
.section-p1{
	padding: 40px 80px;
}
#feature .feature1{
	width: 180px;
	text-align: center;
	padding: 25px 15px;
	box-shadow: 20px 20px 34px rgba(0,0,0,0.03);
	border: 1px solid #cce7d0; 
	border-radius: 4px;
	margin: 15px 0;
}
#feature .feature1:hover {
	box-shadow: 10px 10px 54px rgba(70, 62, 221, 0.1);
}
#feature .feature1 img{
	width:100%;
	margin-bottom: 10px; 
}
#feature .feature1 h6{
	display: inline-block;
	padding: 9px 8px 6px 8px;
	line-height: 1;
	border-radius: 4px;
	color: #088178;
	background-color: #fddde4;
	cursor: pointer;
}
#feature{
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-wrap: wrap;
}
#feature .feature1:nth-child(2) h6{
	background-color: red;
	color: black;
}
#feature .feature1:nth-child(3) h6{
	background-color: grey;
	color: white;
}
#feature .feature1:nth-child(4) h6{
	background-color: green;
	color: white
}
#feature .feature1:nth-child(5) h6 {
	background-color: yellow;
}
#product1{
	text-align: center;
}
#product1 .pro-container{
	display: flex;
	justify-content: space-between;
	padding-top: 20px;
	flex-wrap: wrap;
}
#product1 .pro{
	width: 20%;
	min-width: 250px;
	padding: 10px 12px;
	border: 1px solid #cce7d0;
	border-radius: 25px;
	cursor: pointer;
	box-shadow: 20px 20px 30px rgba(0,0,0,0.02);
	margin: 15px 0; 
	transition: 0.2s ease;
	position: relative;
	
}
#product1 .pro:hover{
	box-shadow: 20px 20px 30px rgba(0,0,0,0.05);
}
#product1 .pro img{
	width :100%;
	border-radius: 15px;
}
#product1 .pro .des{
	text-align: start;
	padding: 10px 0;
}
#product1 .pro .des span{
	color: #606063;
	font-size: 12px;
}
#product1 .pro .des h5{
	padding-top: 7px;
	color: #1a1a1a;
	font-size: 14px;
}
#product1 .pro .des i{
	color: rgba(243, 181, 25);
	font-size: 12px;
}
#product1 .pro .des h4{
	font-size: 15px;
	color: #088178;
	font-weight: 700;
}
#product1 .pro .cart{
	width: 40px;
	height: 40px;
	background-color: #e8f6ea;
	line-height: 40px;
	border-radius: 50px;
	font-weight: 500;
	color: #088178;
	position: absolute;
	bottom: 20px;
	right: 10px;
}
.section-m1{
	margin: 40px 0;
}
#banner{
	display: flex;
	flex-direction: column;
	justify-content:center;
	text-align: center;
	align-items:center;
	background-image: url(../images/banner/b2.jpg);
	width: 100%;
	height: 40vh;
	background-size: cover;
	backgroud-position: cover;
}
#banner h4{
	color: #fff;
	font-size: 14px;
}
#banner h2{
	color: #fff;
	font-size: 30px;
	padding: 10px 0;
}
#banner h2 span{
	color: red;
}
#banner button{
	font-size: 14px;
	font-weight: 600;
	padding: 15px 30px;
	color: #000;
	background-color: #fff;
	border-radius: 4px;
	cursor: pointer;
	border: none;
	transition: 0.2s;
	outline: none;
}
#banner button:hover{
	background: #088178;
	coor: #fff;
}
#sm-banner{
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}
#sm-banner .banner-box{
	display: flex;
	flex-direction: column;
	justify-content:center;
	align-items:flex-start;
	padding: 30px;
	background-image: url(../images/banner/b17.jpg);
	min-width: 520px;
	height: 50vh;
	background-size: cover;
	backgroud-position: center;
}
#sm-banner .banner-box2{
	background-image: url(../images/banner/b10.jpg)
}
#sm-banner h4{
	color: #fff;
	font-size: 20px;
	font-weight: 300;
}
#sm-banner h2{
	color: #fff;
	font-size: 50px;
	font-weight: 900;
}
#sm-banner span{
	color: #fff;
	font-size: 14px;
	font-weight: 900;
	padding-bottom: 15px;
}
#sm-banner .white{
	font-size: 14px;
	font-weight: 400;
	padding: 10px 20px;
	color: #fff;
	background-color: transparent;
	cursor: pointer;
	border: 1px solid #fff;
	transition: 0.5s;
	outline: none;
}
#sm-banner .banner-box:hover button{
	background: #088178;
	border: 1px solid #088178;
	
}
#sm-banner .banner-box3{
	display: flex;
	flex-direction: column;
	justify-content:center;
	align-items:flex-start;
	padding: 30px;
	background-image: url(../images/banner/b7.jpg);
	min-width: 320px;
	height: 50vh;
	margin: 20px 0;
	background-size: cover;
	backgroud-position: center;
}
#sm-banner #banner-box4{
	background-image: url(../images/banner/b16.jpg);
	background-position: right 60%;
}
#sm-banner #banner-box5{
	background-image: url(../images/banner/b19.jpg);
}
#sm-banner .banner-box3 span{
	color: black;
}
#newsletter{
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
	align-items: center;
	background-image: url(../images/banner/b14.png);
	background-position: 20% 30%;
	background-color: #041e42;
	backround-repeat: no-repeat;
	
}
#newsletter .form{
	display: flex;
	width: 40%;
}
#newsletter h4{
	color: #fff;
	margin-bottom: 10px;
	font-size: 20px;
	font-weight: 800;
}
#newsletter p{
	color: grey;
	margin-top: 5px;
	font-size: 13px;
}
#newsletter p span{
	color: yellow;
}
#newsletter input{
	height: 3rem;
	padding: 0 1.25em;
	font-size: 14px;
	width: 100%;
	border: 1px solid transparent;
	border-radius: 4px;
	outline: none;
	border-top-right-radius: 0;
	border-bottom-right-radius: 0;
}
#newsletter button{
	background-color: #088178;
	color: #fff;
	white-space: nowrap;
	height: 3.10rem;
	padding: 0 1em;
	border: 3px solid #088178;
	border-top-right-radius: 4px;
	border-bottom-right-radius: 4px;
}
.col img{
	margin-bottom: 25px;
}
.col h4{
	margin-bottom: 15px;
}
.col span{
	font-weight: bold;
}
.col p{
	margin-bottom: 5px;
	font-size: 14px;
}
.col h3{
	margin-top: 25px;
}
.col .follow{
	margin-top: 10px;
	margin-bottom: 70px;
}
.section-flex{
	display: flex;
	justify-content: space-between;
	
}
.col .block{
	text-decoration: none;
	color: grey;
	display: block;
	cursor: pointer;
	margin-bottom: 8px;
	margin-top: 5px;
}